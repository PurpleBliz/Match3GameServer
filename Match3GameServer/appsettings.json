{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Tags": {
      "INFO": true,
      "DEBUG": true,
      "ERROR": true,
      "WARN": true,
      "CRITICAL": true,
      "ENCODE_PAYLOAD": true
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    }
  }
}
